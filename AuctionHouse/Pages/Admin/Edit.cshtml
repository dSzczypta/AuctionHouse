            <div class="form-group">
                <label asp-for="AspNetUsers.AspNetUser.AccessFailedCount" class="control-label"></label>
                <input asp-for="AspNetUsers.AspNetUser.AccessFailedCount" class="form-control" />
                <span asp-validation-for="AspNetUsers.AspNetUser.AccessFailedCount" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
